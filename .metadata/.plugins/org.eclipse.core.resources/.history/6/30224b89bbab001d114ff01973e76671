package ass_303_6;

public class MonsterMain {

	public static void main(String[] args) {
		FireMonster dragon = new FireMonster("Dragahn");
		WaterMonster leviathan = new WaterMonster("Levadon");
		StoneMonster worm = new StoneMonster ("Wyrm");
		
		System.out.println(dragon.attack());
		System.out.println(leviathan.attack());
		System.out.println(worm.attack());
	}

}
