package ass_303_6;

public class MonsterMain {

	public static void main(String[] args) {
		Monster dragon = new FireMonster("Dragahn");
		Monster leviathan = new WaterMonster("Levadon");
		Monster worm = new StoneMonster ("Wyrm");
		
		System.out.println(dragon.attack());
		System.out.println(leviathan.attack());
		System.out.println(worm.attack());
	}

}
